module OperacionesCajero{
	interface Operaciones{
		boolean respuesta(in string usuario,in string contrasenia);
	
	        string retirar (in string id_usuario , in long  cantidad );
	        
	        void depositar(in string id_usuario , in long  cantidad );
	        
	        long  consultar(in string id_usuario );
	        
	         boolean  consultarAdmin(in string nombreUsuario,in string contrasenia );
	        
	        void mensaje(in string nombreUsuario);
	};
	
};
